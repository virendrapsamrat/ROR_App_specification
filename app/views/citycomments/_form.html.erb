<%= form_with(model: [@city, @city.citycomments.build ] , remote: true, id:"do-nothing") do |form| %>

   <p>
       <%= form.hidden_field :user_id, value: current_user.id %>
   </p>

   <p>
      <%= form.label :body %><br>
      <%= form.text_area :body, required: true %>
   </p>

   <p>
      <%= form.submit 'submit' %>
   </p>

<% end %>