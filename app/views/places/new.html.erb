<%= form_for @place do |form| %>
	<p>
		<%= form.label :Place_Name%> <br>
		<%= form.text_field :place_name %>
    <br>
		<%= form.label :Company_Id%><br>
		<%= form.select :company_id, Company.all.collect { |p| [ p.name, p.id ]}, include_blank:true %>
	
	</p>
<%= form.submit %>
<%end%>

<%= link_to 'Back', welcome_index_path %>