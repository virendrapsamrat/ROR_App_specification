<%= form_with scope: :user, url: users_path, local: true do |form| %>

<p>
    <%= form.label :Email %><br>
    <%= form.text_field :email %>
  </p>

<p>
    <%= form.label :First_Name %><br>
    <%= form.text_field :first_name %>
</p>

<p>
    <%= form.label :Last_Name %><br>
    <%= form.text_field :last_name %>
 </p>

 <p>
  <%= form.label :Country_id %><br>
  <%= form.select :country_id, User.all.collect { |p| [ p.country_name, p.id ] }, include_blank: true %>
 </p>

  <p>
  <%= form.label :Role_id %><br>
  <%= form.select :role_id, User.all.collect { |p| [ p.Role_id, p.id ] }, include_blank: true %>
 </p>

  <p>
    <%= form.label :Start_date %><br>
    <%= form.datetime_select :start_date %>
  </p>
 
 <p>
    <%= form.label :End_date %><br>
    <%= form.datetime_select :End_date %>
  </p>

  <p>
    <%= form.label :Monthly_charge %><br>
    <%= form.number_field :monthly_charge %>
  </p>

<p>
   <%= form.label "Image Id" %> <br>
   <%= form.select :image_id, User.all.collect { |p| [ p.image_id, p.id ] }, include_blank: true %>
</p>

  <p>
   <%= form.label :Active %><br>
   <%= form.check_box :active %>
   </p>

   <p>
    <%= form.submit %>
  </p>

<% end %>
<%= link_to 'New company', new_company_path %><br>
<%= link_to 'Back', welcome_index_path %>